{  
    "manifest_version": 2,
    "name": "二维码生成",
    "version": "1.0",
    "description": "生成当前页面二维码。",
    "browser_action": {
        "default_icon": {
            "19": "img/logo.png",
            "38": "img/logo.png"
        },
        "default_title":"二维码生成",
        "default_popup": "popup.html"
    },
    "permissions": [  
        "tabs",
        "activeTab"
    ]  
}  